{
  "Id": "Policy1525770498484",
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowListObjects",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::${bucket}",
      "Principal": {"AWS":"${role}"}
    },
    {
      "Sid": "AllowObjectsCRUD",
      "Effect": "Allow",
      "Action": [
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:PutObject",
        "s3:List*"
      ],
      "Resource": "arn:aws:s3:::${bucket}/*",
      "Principal": {"AWS":"${role}"}
    }
  ]
}